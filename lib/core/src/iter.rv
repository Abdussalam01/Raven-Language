pub trait Iter<T> {
    pub fn next(self) -> T;

    pub fn has_next(self) -> bool;
}

pub struct NumberIter {
    pub current: i64;
    pub end: i64;
}

impl Iter<u64> for NumberIter {
    pub fn next(self) -> i64 {
        self.current = self.current + 1;
        return self.current;
    }

    pub fn has_next(self) -> bool {
        return self.current == self.end;
    }
}